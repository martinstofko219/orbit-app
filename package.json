{
  "name": "orbit-app",
  "version": "1.0.0",
  "description": "A simple orbiting spheres app using AngularJS.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build-css": "node-sass --include-path scss --output-style expanded scss/styles.scss public/css/styles.css",
    "watch-css": "nodemon --ext scss --exec \"npm run build-css\"",
    "prefix-css": "postcss --use autoprefixer --output public/css/styles.css public/css/styles.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonny219/orbit-app.git"
  },
  "keywords": [
    "AngularJS",
    "node.js",
    "scss",
    "animation"
  ],
  "author": "Martin Stofko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sonny219/orbit-app/issues"
  },
  "homepage": "https://github.com/sonny219/orbit-app#readme",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "postcss-cli": "^2.5.2"
  }
}
